{
	"name": "wukong-background-video",
	"displayName": "wukong-background-video",
	"description": "显示黑神话悟空视频",
	"publisher": "dbfu321",
	"repository": "https://github.com/dbfu/wukong-background-video.git",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.10.5"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "background-video.uninstall",
				"title": "background-video.uninstall"
			}
		],
		"configuration": [{
			"title": "background-video",
			"properties": {
				"background-video.opacity": {
					"type": "number",
					"default" : 0.4,
					"description": "透明度"
				},
				"background-video.videoName": {
					"type": "string",
					"default" : "video1.mp4",
					"description": "视频文件名",
					"enum": ["video1.mp4", "video2.mp4"]
				}
			}
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.10.5",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"@vscode/test-electron": "^2.1.2"
	}
}